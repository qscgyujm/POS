{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node server/index.js",
    "devs": "nodemon server/index.js --watch server/",
    "dev:r": "webpack-dev-server",
    "dev:s": "nodemon --exec babel-node ./server/index.js  --watch server/",
    "build:dev": "webpack --mode development",
    "build:dev:server": "rm -rf ./build && webpack --mode development && babel ./server -d ./build && mkdir ./build/public && nodemon ./build/index.js",
    "build:server": "rm -rf ./build && babel ./server -d ./build",
    "build:react": "webpack --mode production",
    "test": "jest --setupFiles dotenv/config --watch",
    "doc": "nodemon --exec babel-node doc/index.js --watch doc/ -e yaml"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@sendgrid/mail": "^7.0.0",
    "@slack/webhook": "^5.0.3",
    "aws-sdk": "^2.814.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.11.1",
    "dotenv": "^8.2.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "file-loader": "^6.0.0",
    "helmet": "^3.22.0",
    "html-webpack-plugin": "^4.0.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mariadb": "^2.3.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemon": "^2.0.2",
    "polished": "^3.5.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "sequelize": "^5.21.6",
    "styled-components": "^5.0.1",
    "supertest": "^4.0.2",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "unique-string": "^2.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^25.3.0"
  }
}
